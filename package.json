{
  "name": "todo-app",
  "version": "1.0.0",
  "description": "Aplicativo de gerenciamento de tarefas com funcionalidades Kanban",
  "main": "dist/server.js",
  "scripts": {
    "start": "react-scripts start",
    "start:3001": "cross-env PORT=3001 react-scripts start",
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "react-scripts build",
    "migrate": "ts-node src/utils/runMigration.ts",
    "lint": "eslint . --ext .ts",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js"
  },
  "keywords": [
    "todo",
    "tarefas",
    "kanban",
    "projetos",
    "produtividade"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.14.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.17.30",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
